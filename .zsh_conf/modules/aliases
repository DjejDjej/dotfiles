#General aliases
    alias neofetch="fastfetch --localip-show-ipv4 false"
    alias ls='ls --color=auto'
    alias ll="ls -ltra"
    alias grep='grep --color=auto'
    alias vim="sudo nvim"
    alias aider='python -m aider'

    #Folder moving aliases
